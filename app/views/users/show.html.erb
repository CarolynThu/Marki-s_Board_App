
<h3> Make a custom board </h3>
<%= form_for @board do |f| %>
Name: <%= f.text_field :name, placeholder: "Name" %>
<%= f.text_field :type, placeholder: "Type" %>
<%= f.text_field :description, placeholder: "Description" %>

<%= f.submit "submit!" %>

<% end %>


<h3> Previous Custom Boards </h3>
<% @boards.each do |b| %>
<li>
  <%= b.name %> <%= b.type %> <%= b.description %>
  <%= link_to 'Edit', edit_board_path(b.id) %>
</li> 
<% end %>



